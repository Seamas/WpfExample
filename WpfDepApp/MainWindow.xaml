<Window x:Class="WpfDepApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfDepApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <TabControl>
        <TabItem Header="HumanControl">
            <local:HumanControl></local:HumanControl>
        </TabItem>
        <TabItem Header="CanvasControl">
            <local:CanvasControl></local:CanvasControl>
        </TabItem>
        <TabItem Header="RouteEventControl">
            <local:RouteEventControl></local:RouteEventControl>
        </TabItem>
    </TabControl>
</Window>
